class Solution {

    private int compare(String[] version1, int n, String[] version2, int m){
        int i = 0;

        while(i<n && i<m){
            int v1 = Integer.parseInt(version1[i]);
            int v2 = Integer.parseInt(version2[i]);

            //System.out.println(v1 + " " + v2);

            if(v1==v2)
                i++;

            else if(v1>v2)
                return 1;

            else if(v2>v1)
                return -1;
        }

        while(i<n){
            int v1 = Integer.parseInt(version1[i]);
            if(v1!=0)
                return 1;
            i++;
        }

        while(i<m){
            int v2 = Integer.parseInt(version2[i]);
            if(v2!=0)
                return -1;
            i++;
        }

        return 0;
    }

    public int compareVersion(String version1, String version2) {
        String[] string1 = version1.split("\\.");
        String[] string2 = version2.split("\\.");

        int n = string1.length;
        int m = string2.length;

        return compare(string1, n , string2, m);
    }
}
