import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc = new Scanner(System.in);
		
		int tests = sc.nextInt();
		while(tests-->0){
		    int n = sc.nextInt();
		    int x = sc.nextInt();
		    int y = sc.nextInt();
		    int count = 0;
		    
		    for(int i=0;i<n;i++){
		        int num = sc.nextInt();
		        if(num>=x && num<=y)
		            count++;
		    }
		    
		    System.out.println(count);
		}
		
		
		sc.close();

	}
}
