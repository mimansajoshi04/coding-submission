import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		while(n-->0){
		    int num = sc.nextInt();
		    
		    System.out.println(
		        (int) Math.pow(Math.floor(Math.sqrt(num)),2)
		    );
		}
		
		sc.close();

	}
}
